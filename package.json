{
  "name": "@zyjared/vitepress-sidebar",
  "type": "module",
  "version": "0.2.10",
  "packageManager": "pnpm@10.4.0",
  "author": "zyj <zyjared@outlook.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zyjared/vitepress-sidebar.git"
  },
  "keywords": [
    "vitepress",
    "sidebar"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "dev": "vitest",
    "build": "unbuild",
    "prepack": "unbuild",
    "release": "nr build && bumpp && pnpm publish"
  },
  "peerDependencies": {
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.2",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.5"
  }
}
